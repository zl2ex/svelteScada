{
	"name": "sveltescada",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"start": "ORIGIN=http://localhost:3000 npx tsx ./server/index.ts",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
