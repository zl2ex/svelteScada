		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/cookie": "^0.6.0",
		"svelte": "^5.0.0-next.26",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@types/express": "^4.17.21",
		"bcrypt": "^5.1.1",
		"chart.js": "^4.4.1",
		"chartjs-adapter-date-fns": "^3.0.0",
		"chartjs-plugin-zoom": "^2.0.1",
		"cors": "^2.8.5",
